{
	"manifest_version": 2,
	"name": "Refocus",
	"description": "This extension delays page loads for user specified
	websites",
	"background": {
		"scripts":["js/background.js"],
		"persistent": false
	},
	"version": "0.0.0",
	"permissions": [
		"webNavigation",
		"tabs",
		"http://*/",
		"https://*/"
	]
}
